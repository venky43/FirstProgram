  </servlet>
   
  <servlet-mapping>
  	<servlet-name>firstHttpServlet</servlet-name>
  	<url-pattern>/firstHttpServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletWithDatabase</servlet-name>
  	<url-pattern>/ServletWithDatabase</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletToData</servlet-name>
